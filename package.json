    "@intlify/runtime": "^9.1.10",
    "@mdi/font": "^7.3.67",
    "@vuepic/vue-datepicker": "^7.1.0",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "maska": "^2.1.10",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "validator": "^13.11.0",
    "vee-validate": "^4.11.8",
    "vite-tsconfig-paths": "^4.2.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.20",
